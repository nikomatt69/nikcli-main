{
  "$schema": "https://json.schemastore.org/package",
  "name": "@nicomatt69/nikcli",
  "version": "1.5.0",
  "description": "NikCLI - Context-Aware AI Development Assistant",
  "author": "nikomatt69",
  "license": "MIT",
  "main": "dist/cli/nikcli",
  "bin": {
    "nikcli": "dist/cli/nikcli"
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0",
    "pnpm": ">=8.0.0",
    "bun": ">=1.3.0"
  },
  "scripts": {
    "start": "bun --bun run src/cli/index.ts",
    "dev": "bun start",
    "clean": "rm -rf dist public/bin",
    "build": "bun scripts/build-with-secrets.mjs",
    "build:start": "bun scripts/build-with-secrets.mjs && ./dist/cli/nikcli",
    "build:all": "bun scripts/build-all-platforms.mjs",
    "package:standalone": "bun scripts/package-standalone.mjs",
    "update:homebrew": "bun scripts/update-homebrew-formula.mjs",
    "prepublishOnly": "bun run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coherence": "bun tests/verify-coherence.js",
    "test:system": "bun tests/verify-system.js",
    "lint": "bunx biome lint src/",
    "format": "bunx biome format --write src/",
    "check": "bunx biome check --write src/",
    "typecheck": "bunx tsc --noEmit --project tsconfig.cli.json",
    "typecheck:watch": "bunx tsc --noEmit --project tsconfig.cli.json --watch",
    "typecheck:strict": "bunx tsc --noEmit --project tsconfig.cli.json --strict",
    "nikctl": "bun --bun run src/cli/nikctl.ts",
    "nikd": "bun --bun run src/cli/nikd.ts",
    "daemon:start": "bun run nikd start",
    "daemon:status": "bun run nikd status",
    "bg:api": "bun --bun run src/cli/background-agents/api/index.ts",
    "bg:start": "bun run bg:api",
    "bg:list": "bun run nikctl bg list",
    "bg:stats": "bun run nikctl bg stats",
    "web:dev": "cd web && bun run dev",
    "web:build": "cd web && bun run build",
    "web:start": "cd web && bun start",
    "docker:build": "docker build -t nikcli-bg .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "config:check": "bun -e console.log(Config check:, process.env.NODE_ENV || not set)",
    "db:init": "mkdir -p database && echo Database directory created",
    "system:diagnose": "bun -e console.log(Node:, process.version, Platform:, process.platform, Arch:, process.arch)",
    "benchmark:setup": "bash benchmarks/setup.sh",
    "benchmark:run": "bunx ts-node benchmarks/openrouter-benchmark.ts",
    "benchmark:quick": "BENCHMARK_SIZE=5 bunx ts-node benchmarks/openrouter-benchmark.ts",
    "benchmark:full": "BENCHMARK_SIZE=164 bunx ts-node benchmarks/openrouter-benchmark.ts",
    "mobile:install": "cd mobile-app && npm install && cd .. && cd src-tauri && cargo build && cd ..",
    "mobile:dev": "npx tauri dev",
    "mobile:build": "npx tauri build",
    "mobile:build:macos": "npx tauri build --target universal-apple-darwin",
    "mobile:build:ios": "npx tauri ios build",
    "mobile:build:android": "npx tauri android build",
    "mobile:ios:dev": "npx tauri ios dev",
    "mobile:android:dev": "npx tauri android dev",
    "menubar:dev": "cd src-tauri && npx tauri dev",
    "menubar:build": "cd src-tauri && npx tauri build",
    "menubar:build:macos": "cd src-tauri && npx tauri build --target universal-apple-darwin",
    "tauri:dev": "cd src-tauri && cargo tauri dev",
    "tauri:build": "cd src-tauri && cargo tauri build",
    "tauri:build:macos": "cd src-tauri && cargo tauri build --target universal-apple-darwin",
    "tauri:build:linux": "cd src-tauri && cargo tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:windows": "cd src-tauri && cargo tauri build --target x86_64-pc-windows-msvc",
    "tauri:frontend:dev": "cd src/tauri-frontend && bun run dev",
    "tauri:frontend:build": "cd src/tauri-frontend && bun run build",
    "tauri:frontend:install": "cd src/tauri-frontend && bun install"
  },
