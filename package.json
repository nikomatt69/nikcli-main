{
  "$schema": "https://json.schemastore.org/package",
  "name": "@nicomatt69/nikcli",
  "version": "0.5.0",
  "description": "NikCLI - Context-Aware AI Development Assistant",
  "author": "nikomatt69",
  "license": "MIT",
  "main": "dist/cli/index.js",
  "bin": "dist/cli/index.js",
  "files": [
    "dist/**",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0",
    "pnpm": ">=8.0.0",
    "bun": ">=1.3.0"
  },
  "scripts": {
    "start": "bun --bun run src/cli/index.ts",
    "dev": "bun start",
    "build": "bun build src/cli/index.ts --compiler=nodejs --target=node --outdir=dist/cli --format=cjs --packages=external --sourcemap",
    "build:vercel": "echo 'Vercel build completed - using serverless functions'",
    "prepublishOnly": "bun run build",
    "build:start": "bun run build && bun dist/cli/index.js",
    "build:binary": "bun scripts/build-all.js",
    "build:pkg": "bun run build && mkdir -p public/bin && pkg dist/cli/index.js --target node18-macos-arm64 --output public/bin/nikcli-macos-arm64",
    "build:pkg:all": "bun run build && mkdir -p public/bin && bun run pkg:macos:arm64 && bun run pkg:macos:x64 && bun run pkg:linux:x64 && bun run pkg:win:x64",
    "pkg:macos:arm64": "bun run pkg dist/cli/index.js --target node18-macos-arm64 --output public/bin/nikcli-aarch64-apple-darwin",
    "pkg:macos:x64": "bun run pkg dist/cli/index.js --target node18-macos-x64 --output public/bin/nikcli-x86_64-apple-darwin",
    "pkg:linux:x64": "bun run pkg dist/cli/index.js --target node18-linux-x64 --output public/bin/nikcli-x86_64-linux",
    "pkg:win:x64": "bun run pkg dist/cli/index.js --target node18-win-x64 --output public/bin/nikcli-x86_64-windows.exe",
    "build:release": "bun scripts/build-release.mjs",
    "build:bun": "mkdir -p public/bin && bun build --compile --minify --sourcemap src/cli/index.ts --packages=external --outfile public/bin/nikcli",
    "build:bun:macos:arm64": "mkdir -p public/bin && bun build --compile --minify --sourcemap --packages=external --target=bun-darwin-arm64 src/cli/index.ts --outfile public/bin/nikcli-aarch64-apple-darwin",
    "build:bun:macos:x64": "mkdir -p public/bin && bun build --compile --minify --sourcemap --packages=external --target=bun-darwin-x64 src/cli/index.ts --outfile public/bin/nikcli-x86_64-apple-darwin",
    "build:bun:linux:x64": "mkdir -p public/bin && bun build --compile --minify --sourcemap --packages=external --target=bun-linux-x64 src/cli/index.ts --outfile public/bin/nikcli-x86_64-linux",
    "build:bun:windows:x64": "mkdir -p public/bin && bun build --compile --minify --sourcemap --packages=external --target=bun-windows-x64 src/cli/index.ts --outfile public/bin/nikcli-x86_64-windows.exe",
    "build:bun:all": "bun run build:bun:macos:arm64 && bun run build:bun:macos:x64 && bun run build:bun:linux:x64 && bun run build:bun:windows:x64",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coherence": "bun tests/verify-coherence.js",
    "test:system": "bun tests/verify-system.js",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/",
    "nikctl": "bun --bun run src/cli/nikctl.ts",
    "nikd": "bun --bun run src/cli/nikd.ts",
    "daemon:start": "bun run nikd start",
    "daemon:status": "bun run nikd status",
    "bg:start": "bun run nikctl bg start",
    "bg:list": "bun run nikctl bg list",
    "bg:stats": "bun run nikctl bg stats",
    "web:dev": "cd web && bun run dev",
    "web:build": "cd web && bun run build",
    "web:start": "cd web && bun start",
    "docker:build": "docker build -t nikcli-bg .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "config:check": "bun -e \"console.log('Config check:', process.env.NODE_ENV || 'not set')\"",
    "db:init": "mkdir -p database && echo 'Database directory created'",
    "system:diagnose": "bun -e \"console.log('Node:', process.version, 'Platform:', process.platform, 'Arch:', process.arch)\""
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/gateway": "^1.0.10",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.66",
    "@ai-sdk/vercel": "^1.0.10",
    "@anthropic-ai/tokenizer": "^0.0.4",
    "@coinbase/agentkit": "^0.10.1",
    "@coinbase/agentkit-vercel-ai-sdk": "^0.1.0",
    "@goat-sdk/adapter-vercel-ai": "^0.2.10",
    "@goat-sdk/plugin-erc20": "^0.2.14",
    "@goat-sdk/plugin-polymarket": "^0.3.14",
    "@goat-sdk/wallet-viem": "^0.3.0",
    "@mozilla/readability": "^0.6.0",
    "@nicomatt69/streamtty": "0.0.1",
    "@octokit/rest": "^20.1.2",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@supabase/supabase-js": "^2.55.0",
    "@types/blessed": "^0.1.25",
    "@types/cli-progress": "^3.11.5",
    "@types/cors": "^2.8.19",
    "@types/diff": "^7.0.2",
    "@types/express": "^4.17.23",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/marked-terminal": "^6.1.1",
    "@types/uuid": "^9.0.8",
    "@upstash/redis": "^1.35.3",
    "@valibot/to-json-schema": "^1.3.0",
    "@vercel/kv": "^1.0.1",
    "@vercel/node": "^5.3.22",
    "ai": "^3.4.33",
    "axios": "^1.12.2",
    "blessed": "^0.1.81",
    "boxen": "^7.1.1",
    "bun": "^1.3.0",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "chromadb": "^3.0.11",
    "cli-highlight": "^2.1.11",
    "cli-progress": "^3.12.0",
    "commander": "^13.1.0",
    "context-interceptor-sdk": "^0.1.1",
    "cors": "^2.8.5",
    "diff": "^8.0.2",
    "dotenv": "^17.2.1",
    "effect": "^3.12.8",
    "express": "5.1.0",
    "express-rate-limit": "^8.0.1",
    "globby": "^14.1.0",
    "gpt-tokenizer": "^3.0.1",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "highlight.js": "^11.11.1",
    "inquirer": "^9.2.12",
    "ioredis": "^5.7.0",
    "js-tiktoken": "^1.0.21",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.25",
    "marked": "^15.0.7",
    "marked-terminal": "^7.3.0",
    "nanoid": "^5.0.4",
    "ollama-ai-provider": "^1.2.0",
    "ora": "^8.0.1",
    "playwright": "^1.56.1",
    "readline": "^1.3.0",
    "shiki": "^3.13.0",
    "sury": "^10.0.0",
    "task-master-ai": "^0.26.0",
    "terminal-image": "^4.0.0",
    "uuid": "11.1.0",
    "viem": "^2.37.7",
    "vscode-jsonrpc": "^8.2.1",
    "ws": "^8.18.3",
    "yaml": "^2.8.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/glob": "^8.1.0",
    "@types/gradient-string": "^1.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.14",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.9",
    "eslint-plugin-unused-imports": "^4.2.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}