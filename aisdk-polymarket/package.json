{
  "name": "@bamby/aisdk-polymarket",
  "version": "0.1.0",
  "description": "AI SDK provider for Polymarket CLOB trading with Coinbase CDP wallet integration",
  "author": "Bamby <nikomatt69>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc --out docs src/index.ts",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test && npm run build",
    "prepare": "husky install",
    "example:place-order": "tsx examples/node-place-order.ts",
    "example:agent": "tsx examples/agent-trader.ts",
    "example:production": "tsx examples/advanced-production.ts"
  },
  "dependencies": {
    "@coinbase/cdp-sdk": "^0.16.0",
    "ai": "^3.4.33",
    "@ai-sdk/openai": "^1.0.66",
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "zod": "^3.22.4",
    "ethers": "^6.13.0",
    "viem": "^2.37.7",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.9.2",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "vitest": "^3.2.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "prettier": "^3.2.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "typedoc": "^0.25.7"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "polymarket",
    "ai-sdk",
    "vercel",
    "coinbase",
    "cdp",
    "trading",
    "clob",
    "prediction-markets",
    "eip-712",
    "ethereum"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nikomatt69/nikcli-main.git",
    "directory": "aisdk-polymarket"
  },
  "publishConfig": {
    "access": "public"
  }
}
