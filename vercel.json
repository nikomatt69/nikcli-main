{
  "functions": {
    "api/github/webhook.ts": {
      "maxDuration": 60,
      "memory": 512
    },
    "api/health.ts": {
      "maxDuration": 10
    },
    "api/subscription/webhook.ts": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/subscription/status.ts": {
      "maxDuration": 10
    },
    "api/openrouter/provision.ts": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "rewrites": [
    {
      "source": "/api/v1/web/github/webhook",
      "destination": "/api/github/webhook"
    }
  ]
}