{
    "name": "context-interceptor-sdk",
    "version": "0.1.1",
    "description": "RAG-based context injection SDK for OpenAI-compatible providers and Vercel AI SDK",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        ".env.example",
        "GETTING_STARTED.md",
        "SIMPLE_INTEGRATION.md",
        "PROVIDER_ARCHITECTURE.md"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./quick-setup": {
            "types": "./dist/quick-setup.d.ts",
            "require": "./dist/quick-setup.js",
            "import": "./dist/quick-setup.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:run": "vitest run",
        "clean": "rm -rf dist",
        "prebuild": "npm run clean",
        "prepublishOnly": "npm run typecheck && npm run build"
    },
    "keywords": [
        "rag",
        "context",
        "llm",
        "openai",
        "ai-sdk",
        "upstash",
        "embeddings",
        "vector",
        "semantic-search",
        "retrieval"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@ai-sdk/openai": "^1.0.0",
        "@upstash/redis": "^1.35.3",
        "@upstash/vector": "^1.1.5",
        "ai": "^4.0.0",
        "fast-glob": "^3.3.3",
        "lru-cache": "^11.0.2",
        "nanoid": "^5.0.4",
        "openai": "^4.77.0",
        "pino": "^9.5.0",
        "tiktoken": "^1.0.18",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nikomatt69/context-interceptor-sdk"
    },
    "bugs": {
        "url": "https://github.com/nikomatt69/context-interceptor-sdk/issues"
    },
    "homepage": "https://github.com/nikomatt69/context-interceptor-sdk#readme"
}
